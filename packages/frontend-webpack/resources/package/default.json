  "author": {
    "name": "Ciffi"
  },
  "description": "Frontend Base Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciffi/ciffi-js.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ciffi/ciffi-js",
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-do-expressions": "7.6.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-regenerator": "7.4.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.6.2",
    "@ciffi-js/builder": "4.5.3",
    "@ciffi-js/jsonize-path": "1.1.1",
    "@ciffi-js/webpack-wcs-loader": "0.0.4",
    "autoprefixer": "9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "clean-css-cli": "4.3.0",
    "copyfiles": "2.1.1",
    "css-loader": "3.2.0",
    "error-overlay-webpack-plugin": "0.4.1",
    "eslint": "6.5.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-react": "7.15.1",
    "file-loader": "4.2.0",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "livereload": "0.8.2",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss-cli": "6.1.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.1.2",
    "twig": "1.13.3",
    "twig-loader": "0.5.1",
    "webpack": "4.41.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.6.0",
    "@ciffi-js/device": "1.1.2",
    "breakpoint-sass": "2.7.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "reset-css": "4.0.1",
    "styled-components": "4.4.0"
  },
  "scripts": {
    "start": "ciffi-builder start --env",
    "dev": "ciffi-builder dev --env",
    "build": "ciffi-builder build --env",
    "serve": "ciffi-builder serve --env",
    "jsonize": "ciffi-jsonize",
    "config": "ciffi-builder config --env",
    "assets": "ciffi-builder assets",
    "newcomponent": "./bin/new-component.sh $1 $2",
    "prettier": "prettier --write --single-quote '**/*.{jsx,js,json,scss}'",
    "test-dev": "jest --watchAll",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "browserslist": ["last 4 versions"],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "Config": "<rootDir>/src/config/config.js",
      "Theme": "<rootDir>/src/config/theme.js",
      ".+\\.(css|scss)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/scripts/**/*.{js,jsx}",
      "!<rootDir>/src/config/**/*.{js,jsx}"
    ]
  }
}
