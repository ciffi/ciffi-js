{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "jquery": true,
    "mocha": true,
    "es6": true
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {},
  "rules": {
    "no-console": 0,
    "react/prop-types": 0,
    "strict": [2, "safe"],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": 0
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.12.0"
    }
  }
}
