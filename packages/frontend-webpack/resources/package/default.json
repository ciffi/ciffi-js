    "babel-plugin-module-resolver": "3.1.2",
    "babel-plugin-styled-components": "1.10.0",
    "clean-css-cli": "4.2.1",
    "copyfiles": "2.1.0",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "livereload": "0.7.0",
    "node-sass": "4.11.0",
    "postcss-cli": "6.1.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.9.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "twig": "1.13.0",
    "twig-loader": "0.4.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.28.4",
    "webpack-cli": "3.1.1",
    "webpack-dev-middleware": "3.5.1",
    "webpack-hot-middleware": "2.24.3",
    "workbox-webpack-plugin": "3.6.3"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@ciffi-js/device": "1.0.2",
    "breakpoint-sass": "2.7.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "reset-css": "2.2.1",
    "styled-components": "4.1.3"
  },
  "scripts": {
    "start": "ciffi-builder start --env",
    "dev": "ciffi-builder dev --env",
    "build": "ciffi-builder build --env",
    "serve": "ciffi-builder serve --env",
    "jsonize": "ciffi-jsonize",
    "config": "ciffi-builder config --env",
    "assets": "ciffi-builder assets",
    "newcomponent": "./bin/new-component.sh $1 $2",
    "prettier": "prettier --write --single-quote '**/*.{jsx,js,json,scss}'",
    "precommit": "pretty-quick --staged",
    "test-dev": "jest --watchAll",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "Config": "<rootDir>/src/scripts/config/config.js",
      "Theme": "<rootDir>/src/scripts/config/theme.js",
      ".+\\.(css|scss)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/scripts/**/*.{js,jsx}",
      "!<rootDir>/src/scripts/config/**/*.{js,jsx}"
    ]
  }
}
